{
  "manifest_version": 2,

  "name": "A brower action with a popup that changes the page color",
  "description": "Change the current page color",
  "version": "1.0",
  "permissions": [
  "activeTab", "http://*/*", "http://*/*"
  ],


  "browser_action": {
    "default_icon": "icon.png",
    "default_popup":"popup.html",
    "default_title": "Click To Change Color!"
  }
}
